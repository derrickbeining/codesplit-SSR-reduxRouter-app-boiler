[ignore]
.*/git/.*
.*/node_modules/fbjs/.*
.*/node_modules/webpack-flush-chunks/*
[include]

[libs]

[lints]

[options]
;; tells Flow where to look up types for CSS/SCSS modules made possible by webpack
module.name_mapper.extension='.+\(.s?css\)' -> '<PROJECT_ROOT>/flow-typed/CSSModule'
module.system.node.resolve_dirname=server
module.system.node.resolve_dirname=src
module.system.node.resolve_dirname=node_modules
module.file_ext=.js
module.file_ext=.json
module.file_ext=.css
module.file_ext=.scss
[strict]
